<script async src="https://www.googletagmanager.com/gtag/js?id=G-MS96FEG26P"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-MS96FEG26P');
    @if (!MultiTenantHelpers.GetTenantGoogleAnalyticsTrackingCode().IsNullOrWhiteSpace())
    {
        @Html.Raw(String.Format("gtag('config', '{0}');", MultiTenantHelpers.GetTenantGoogleAnalyticsTrackingCode()))
    }
</script>

